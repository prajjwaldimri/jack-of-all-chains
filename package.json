{
  "name": "jack-of-all-chains",
  "version": "0.1.0",
  "description": "A blockchain that can analyze various consensus mechanisms",
  "main": "src/server/app.js",
  "scripts": {
    "start-blockchain": "tsnd --respawn --transpile-only --debug src/blockchain/index.ts",
    "compile-blockchain": "rimraf ./build && tsc",
    "start-server": "tsnd --respawn --transpile-only src/server/app.ts",
    "start-peer": "cross-env GENERATE_PEER_PORT='true' tsnd --respawn --transpile-only src/server/app.ts",
    "start-redis": "redis-server --daemonize yes",
    "test": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prajjwaldimri/jack-of-all-chains.git"
  },
  "author": "Prajjwal Dimri",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/prajjwaldimri/jack-of-all-chains/issues"
  },
  "homepage": "https://github.com/prajjwaldimri/jack-of-all-chains#readme",
  "devDependencies": {
    "@types/bent": "^7.3.0",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.8",
    "@types/elliptic": "^6.4.12",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.2",
    "@types/npm": "^2.0.31",
    "@types/pubsub-js": "^1.8.1",
    "chalk": "^4.1.0",
    "jest": "^26.4.2",
    "npm": "^6.14.8",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.0",
    "ts-node-dev": "^1.0.0-pre.63",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@types/ws": "^7.2.7",
    "bent": "^7.3.10",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "elliptic": "^6.5.3",
    "express": "^4.17.1",
    "nanoid": "^3.1.12",
    "rfdc": "^1.1.4",
    "sha3": "^2.1.3",
    "ws": "^7.3.1"
  }
